{
  "name": "klib/laravel-log-reader",
  "description": "A simple and beautiful laravel log reader",
  "license": "cc-by-4.0",
  "keywords": [
    "laravel",
    "log",
    "reader"
  ],
  "type": "library",
  "authors": [
    {
      "name": "linhai",
      "email": "kami@cancanyou.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Haruncpi\\LaravelLogReader\\": "src"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Haruncpi\\LaravelLogReader\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
